{
  "name": "@nitric/cli",
  "description": "CLI tool for nitric applications",
  "version": "0.0.53",
  "author": "Nitric <https://github.com/nitric-dev>",
  "bin": {
    "nitric": "./bin/run"
  },
  "bugs": "https://github.com/packages/cli/issues",
  "dependencies": {
    "@nitric/cli-common": "workspace:*",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/plugin-plugins": "^1.9.5",
    "chalk": "^4.1.0",
    "cli-ux": "^5.5.0",
    "common-tags": "^1.8.0",
    "dockerode": "^3.2.1",
    "enquirer": "^2.3.0",
    "eventemitter3": "^4.0.7",
    "execa": "^4.0.3",
    "get-port": "^5.1.1",
    "inquirer": "^7.3.3",
    "listr2": "^3.6.2",
    "multimatch": "^5.0.0",
    "node-emoji": "^1.10.0",
    "os": "^0.1.1",
    "rimraf": "^3.0.2",
    "simple-git": "^2.31.0",
    "stream-to-promise": "^3.0.0",
    "tar-fs": "^2.1.0",
    "tslib": "^1",
    "which": "^2.0.2",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.26.0",
    "@types/dockerode": "^2.5.34",
    "@types/jest": "^26.0.23",
    "@types/node": "^10",
    "@types/node-emoji": "^1.8.1",
    "@types/rimraf": "^3.0.0",
    "@types/stream-to-promise": "^2.2.1",
    "@types/tar-fs": "^2.0.0",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "jest": "^26.6.1",
    "licensee": "^8.2.0",
    "pkg": "^4.4.9",
    "ts-jest": "^26.4.3",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/packages/cli",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "topics": {
      "templates": {
        "description": "Commands for template management"
      },
      "templates:repos": {
        "description": "Commands for managing template repositories"
      },
      "make": {
        "description": "Commands to make new projects and assets."
      },
      "deploy": {
        "description": "Plugin commands to deploy on supported providers. E.g. `$ nitric plugins:install @nitric/plugin-aws`"
      },
      "down": {
        "description": "Plugin commands to tear down deployments on supported providers. E.g. `$ nitric plugins:install @nitric/plugin-aws`"
      }
    },
    "bin": "nitric",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  },
  "repository": "packages/cli",
  "scripts": {
    "license:check": "licensee --production",
    "prepackage": "rm -rf lib && tsc -p tsconfig.build.json && oclif-dev manifest && oclif-dev readme",
    "postpackage": "rm -f oclif.manifest.json",
    "package:linux": "yarn run prepack && pkg . --output ./dist/nitric-linux-x64 -t node12-linux-x64",
    "package:macos": "yarn run prepack && pkg . --output ./dist/nitric-macos-x64 -t node12-macos-x64",
    "package:win": "yarn run prepack && pkg . --output ./dist/nitric-win-x64.exe -t node12-win-x64",
    "package:all": "yarn run package:linux && yarn run package:macos && yarn run package:win",
    "package": "yarn run prepackage && yarn run package:all && yarn run postpackage",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc --project tsconfig.build.json && oclif-dev manifest && oclif-dev readme",
    "test": "tsc --emitDeclarationOnly && jest",
    "npm:publish": "yarn npm publish --access public --tolerate-republish",
    "version": "oclif-dev readme && git add README.md",
    "nitric": "./bin/run"
  },
  "pkg": {
    "scripts": "lib/**/*.js",
    "assets": [
      "package.json",
      "../../node_modules/yarn/**/*"
    ]
  },
  "types": "lib/index.d.ts"
}
