{
  "name": "@nitric/cli",
  "description": "CLI tool for nitric applications",
  "version": "0.0.9",
  "author": "Nitric <https://github.com/nitric-dev>",
  "bin": {
    "nitric": "./bin/run"
  },
  "bugs": "https://github.com/packages/cli/issues",
  "dependencies": {
    "@nitric/cli-common": "workspace:*",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/plugin-plugins": "^1.9.0",
    "clear": "^0.1.0",
    "cli-ux": "^5.5.0",
    "dockerode": "^3.2.1",
    "eventemitter3": "^4.0.7",
    "execa": "^4.0.3",
    "get-port": "^5.1.1",
    "inquirer": "^7.3.3",
    "keypress": "^0.2.1",
    "listr": "^0.14.3",
    "multimatch": "^5.0.0",
    "os": "^0.1.1",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "stream-to-promise": "^3.0.0",
    "tar-fs": "^2.1.0",
    "tslib": "^1",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/dockerode": "^2.5.34",
    "@types/jest": "^26.0.15",
    "@types/listr": "latest",
    "@types/node": "^10",
    "@types/rimraf": "^3.0.0",
    "@types/stream-to-promise": "^2.2.1",
    "@types/tar-fs": "^2.0.0",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "jest": "^26.6.1",
    "ts-jest": "^26.4.3",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/packages/cli",
  "keywords": [
    "oclif"
  ],
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "topics": {
      "make": {
        "description": "Commands to make new projects and assets."
      },
      "deploy": {
        "description": "Plugin commands to deploy on supported providers. E.g. `$ nitric plugins:install @nitric/plugin-aws`"
      },
      "down": {
        "description": "Plugin commands to tear down deployments on supported providers. E.g. `$ nitric plugins:install @nitric/plugin-aws`"
      }
    },
    "bin": "nitric",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  },
  "repository": "packages/cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "version": "oclif-dev readme && git add README.md",
    "nitric": "./bin/run"
  },
  "types": "lib/index.d.ts"
}
